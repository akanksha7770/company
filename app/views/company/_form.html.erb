<%#= form_for @company do |f| %>
 
  <%= form_for :company, url: company_index_path do |f| %>
 
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :country_id %><br>
    <%= f.text_area :country_id %>
  </p>
  
   <p>
    <%= f.label :state_id %><br>
    <%= f.text_area :state_id %>
  </p>

  <p>
    <%= f.label :city_id %><br>
    <%= f.text_area :city_id %>
  </p>

  <p>
    <%= f.label :postcode %><br>
    <%= f.text_area :postcode %>
  </p>

   <p>
    <%= f.label :start_date %><br>
    <%= f.date_select :start_date %>
  </p>

   <p>
    <%= f.label :active %><br>
    <%#= f.text_area :active %>
    <%= f.check_box :active %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
 
<% end %>