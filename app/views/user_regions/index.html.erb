<h1>Listing user_regions</h1>
<%= link_to 'New user_region', new_user_region_path %>
 
<table>
  <tr>
    <th>User_id</th>
    <th>Region_id</th>
  </tr>
 
  <% @user_regions.each do |user_regions| %>
    <tr>
      <td><%= user_regions.user_id %></td>
      <td><%= user_regions.region_id %></td>
      <td><%= link_to 'Show', user_region_path(user_regions) %></td>
      <td><%= link_to 'Edit', edit_user_region_path(user_regions) %></td>
      <td><%= link_to 'Destroy', user_region_path(user_regions),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>