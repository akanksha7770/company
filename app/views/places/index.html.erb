<h1>Listing places</h1>
 
<%= link_to 'My places', controller: 'places' %>
<%= link_to 'New places', new_place_path %>
<table>
  <tr>
    
    <th>Place_name</th>
    <th>Company_id</th>
    <th colspan="2"></th>
  </tr>
 
  <% @places.each do |places| %>
    <tr>
      
      <td><%= places.place_name %></td>
      <td><%= places.company_id %></td>
      <td><%= link_to 'Show', place_path(places) %></td>
      <td><%= link_to 'Edit', edit_place_path(places) %></td>
      <td><%= link_to 'Destroy', place_path(places),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>

    </tr>
  <% end %>
</table>


