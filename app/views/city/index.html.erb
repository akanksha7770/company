<h1>Listing city</h1>
 
<%= link_to 'My city', controller: 'city' %>
<%= link_to 'New city', new_city_path %>
<table>
  <tr>
    <th>city_name</th>
    <th colspan="2"></th>
  </tr>
 
  <% @city.each do |city| %>
    <tr>
      <td><%= city.city_name %></td>
      <td><%= link_to 'Show', city_path(city) %></td>
      <td><%= link_to 'Edit', edit_city_path(city) %></td>
      <td><%= link_to 'Destroy', city_path(city),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>

    </tr>
  <% end %>
</table>


